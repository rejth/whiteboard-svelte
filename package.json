{
  "name": "lerboardx-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "husky install"
  },
  "commit-msg": {
    "regex": "(feat|fix|docs|style|refactor|test|chore)(:\\s)([a-z]{1,})",
    "error-message": "Need message like: feat: add test for new page. Read Semantic Commit Messages"
  },
  "lint-staged": {
    "*.{ts,svelte}": [
      "npm run format:fix",
      "npm run lint:fix"
    ],
    "*.{css,scss,json,svg,js,cjs,html,md}": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.4",
    "@tsconfig/svelte": "^4.0.1",
    "@types/node": "^20.2.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "commit-msg-regex": "^1.2.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.62.1",
    "svelte-check": "^3.3.1",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  },
  "dependencies": {
    "bezier-js": "^6.1.3",
    "svelte": "^3.59.1",
    "uuid": "^9.0.0"
  }
}
